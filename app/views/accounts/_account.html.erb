<div id="<%= dom_id account %>">
  <p>
    <strong>Email:</strong>
    <%= account.email %>
  </p>

  <p>
    <strong>First name:</strong>
    <%= account.first_name %>
  </p>

  <p>
    <strong>Last name:</strong>
    <%= account.last_name %>
  </p>

  <p>
    <strong>Birthday:</strong>
    <%= account.birthday %>
  </p>

  <p>
    <strong>Company:</strong>
    <%= account.company %>
  </p>

  <p>
    <strong>Position:</strong>
    <%= account.position %>
  </p>

</div>
